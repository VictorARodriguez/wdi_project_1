<%= form_for game_local do |f| %>
  <%= f.text_field :name, {placeholder: "name"} %>
  <%= f.text_field :image_one_url, {placeholder: "image_one_url"} %>
  <%= f.text_field :image_two_url, {placeholder: "image_two_url"} %>
  <%= f.text_field :image_three_url, {placeholder: "image_three_url"} %>
  <%= f.submit %>
<% end %>